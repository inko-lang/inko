# Types for cryptographic ciphers.

# A cryptographic cipher.
trait pub Cipher {
  # Encrypts a `ByteArray` and returns the encrypted result as a new
  # `ByteArray`.
  fn pub mut encrypt(bytes: ref ByteArray) -> ByteArray

  # Decrypts a `ByteArray` and returns the decrypted result as a new
  # `ByteArray`.
  fn pub mut decrypt(bytes: ref ByteArray) -> ByteArray
}
