    t.equal((1,), (1,))
  }

  t.test('Tuple1.hash') fn (t) {
    t.equal(hash((1,)), hash((1,)))
  }

  t.test('Tuple1.fmt') fn (t) {
    t.equal(fmt((1,)), '(1)')
  }

