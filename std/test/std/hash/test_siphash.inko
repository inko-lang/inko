fn pub tests(t: mut Tests) {
  t.test('SipHasher13.finish') fn (t) {
    let buf = []

    EXPECTED.iter.each_with_index fn (index, exp) {
      let hasher = SipHasher13.new(KEY0, KEY1)

      buf.iter.each fn (v) { hasher.write(v) }
      t.equal(hasher.finish, exp)
      buf.push(index)
    }
  }
}
